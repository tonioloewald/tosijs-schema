{
  "name": "tosijs-schema",
  "version": "0.0.5",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "typescript",
    "javascript",
    "json",
    "schema",
    "validation",
    "inference"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tonioloewald/tosijs-schema.git"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5",
    "zod": "^4.1.12"
  },
  "scripts": {
    "pack": "bun test && bun bench.ts && bun build index.ts --outdir=dist && tsc index.ts --declaration --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "bun run pack",
    "latest": "rm -rf node_modules && bun install"
  }
}
