{
  "name": "tosijs-schema",
  "version": "0.0.2",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tonioloewald/tosijs-schema.git"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5"
  },
  "scripts": {
    "pack": "bun test && bun build index.ts --outdir=dist && tsc index.ts --declaration --emitDeclarationOnly --outDir dist",
    "test": "bun test",
    "prepublishOnly": "bun run pack",
    "latest": "rm -rf node_modules && bun install"
  }
}
